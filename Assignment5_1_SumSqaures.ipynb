    "class Point:\n",
    "    sqsum_=0\n",
    "    def __init__(self,x,y,z):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "        self.z = z\n",
    "\n",
    "    def sqSum(self):\n",
    "        sqsum_=(self.x*self.x)+(self.y*self.y)+(self.z*self.z)\n",
    "        return sqsum_\n",
    "obj1= Point(1,3,5)\n",
    "val=obj1.sqSum()\n",
    "print(val)\n",
    "        "
   ]
